{
    "name": "dachcom-digital/members",
    "type": "pimcore-bundle",
    "license": "GPL-3.0-or-later",
    "description": "Pimcore User, Object, Asset and Document Authentication",
    "keywords": ["pimcore", "members", "authentication"],
    "homepage": "https://github.com/dachcom-digital/pimcore-members",
    "authors": [
        {
            "name": "DACHCOM.DIGITAL Stefan Hagspiel",
            "email": "shagspiel@dachcom.ch",
            "homepage": "http://www.dachcom.com/",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "MembersBundle\\": "src/MembersBundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/TestKernel.php"
        ]
    },
    "extra": {
        "pimcore": {
            "bundles": [
                "MembersBundle\\MembersBundle"
            ]
        }
    },
    "require": {
        "pimcore/pimcore": "^6.0"
    },
    "require-dev": {
        "codeception/codeception": "~2.4.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "symplify/easy-coding-standard": "^5.4"
    },
    "suggest": {
        "knpuniversity/oauth2-client-bundle": "^1.0"
    }
}
